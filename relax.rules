\START-GROUP TIME
  MIDNIGHT
  MORNING
  MIDDAY
  AFTERNOON
  EVENING
\END-GROUP

\START-GROUP DURATION
  FLASHING
  FAST
  SHORT
  AVERAGE
  LONG
  EXTENSIVE
  UNFINISHED
\END-GROUP

\START-GROUP VARIETY
  NONE
  POOR
  AVERAGE
  RICH
\END-GROUP

\START-GROUP NOISE
  SILENT
  NOISY
  TURBULENT
\END-GROUP

\START-GROUP CONFIDENCE
  ILLOGIC
  UNLIKELY
  UNCERTAIN
  LIKELY
  CERTAIN
\END-GROUP

\START-MAPPING
  TIME
  DURATION
  NOISE
  VARIETY
  \INTO CONFIDENCE
\END-MAPPING

\RULE * FLASHING       *             * ILLOGIC
\RULE * *              TURBULENT     * UNLIKELY
\RULE * FAST           SILENT        * LIKELY
\RULE * FAST           NOISY         * UNCERTAIN
\RULE * SHORT          SILENT        * LIKELY
\RULE * SHORT          NOISY         * CERTAIN
\RULE * AVERAGE        SILENT        * UNCERTAIN
\RULE * AVERAGE        NOISY         * CERTAIN
\RULE * LONG           SILENT        * UNCERTAIN
\RULE * LONG           NOISY         * CERTAIN
\RULE * EXTENSIVE      SILENT        * UNCERTAIN
\RULE * EXTENSIVE      NOISY         * CERTAIN
\RULE * UNFINISHED     SILENT        * UNCERTAIN
\RULE * UNFINISHED     NOISY         * UNCERTAIN
