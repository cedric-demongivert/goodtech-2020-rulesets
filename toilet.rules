\START-GROUP TIME
  MIDNIGHT
  MORNING
  MIDDAY
  AFTERNOON
  EVENING
\END-GROUP

\START-GROUP DURATION
  FLASHING
  FAST
  SHORT
  AVERAGE
  LONG
  EXTENSIVE
  UNFINISHED
\END-GROUP

\START-GROUP VARIETY
  NONE
  POOR
  AVERAGE
  RICH
\END-GROUP

\START-GROUP NOISE
  SILENT
  NOISY
  TURBULENT
\END-GROUP

\START-GROUP CONFIDENCE
  ILLOGIC
  UNLIKELY
  UNCERTAIN
  LIKELY
  CERTAIN
\END-GROUP

\START-MAPPING
  TIME
  DURATION
  NOISE
  VARIETY
  \INTO CONFIDENCE
\END-MAPPING

\RULE *             FLASHING      *             * ILLOGIC
\RULE *             LONG          *             * ILLOGIC
\RULE *             EXTENSIVE     *             * ILLOGIC
\RULE *             *             TURBULENT     * ILLOGIC
\RULE MIDNIGHT      FAST          SILENT        * CERTAIN
\RULE MIDNIGHT      FAST          NOISY         * CERTAIN
\RULE MIDNIGHT      SHORT         SILENT        * CERTAIN
\RULE MIDNIGHT      SHORT         NOISY         * CERTAIN
\RULE MIDNIGHT      AVERAGE       SILENT        * CERTAIN
\RULE MIDNIGHT      AVERAGE       NOISY         * CERTAIN
\RULE MIDNIGHT      UNFINISHED    SILENT        * LIKELY
\RULE MIDNIGHT      UNFINISHED    NOISY         * LIKELY
\RULE MORNING       FAST          SILENT        * CERTAIN
\RULE MORNING       FAST          NOISY         * CERTAIN
\RULE MORNING       SHORT         SILENT        * LIKELY
\RULE MORNING       SHORT         NOISY         * LIKELY
\RULE MORNING       AVERAGE       SILENT        * UNCERTAIN
\RULE MORNING       AVERAGE       NOISY         * UNCERTAIN
\RULE MORNING       UNFINISHED    SILENT        * LIKELY
\RULE MORNING       UNFINISHED    NOISY         * UNCERTAIN
\RULE MIDDAY        FAST          SILENT        * CERTAIN
\RULE MIDDAY        FAST          NOISY         * CERTAIN
\RULE MIDDAY        SHORT         SILENT        * CERTAIN
\RULE MIDDAY        SHORT         NOISY         * LIKELY
\RULE MIDDAY        AVERAGE       SILENT        * CERTAIN
\RULE MIDDAY        AVERAGE       NOISY         * LIKELY
\RULE MIDDAY        UNFINISHED    SILENT        * LIKELY
\RULE MIDDAY        UNFINISHED    NOISY         * UNCERTAIN
\RULE AFTERNOON     FAST          SILENT        * CERTAIN
\RULE AFTERNOON     FAST          NOISY         * LIKELY
\RULE AFTERNOON     SHORT         SILENT        * CERTAIN
\RULE AFTERNOON     SHORT         NOISY         * LIKELY
\RULE AFTERNOON     AVERAGE       SILENT        * LIKELY
\RULE AFTERNOON     AVERAGE       NOISY         * UNCERTAIN
\RULE AFTERNOON     UNFINISHED    SILENT        * LIKELY
\RULE AFTERNOON     UNFINISHED    NOISY         * UNCERTAIN
\RULE EVENING       FAST          SILENT        * CERTAIN
\RULE EVENING       FAST          NOISY         * CERTAIN
\RULE EVENING       SHORT         SILENT        * LIKELY
\RULE EVENING       SHORT         NOISY         * LIKELY
\RULE EVENING       AVERAGE       SILENT        * UNCERTAIN
\RULE EVENING       AVERAGE       NOISY         * UNCERTAIN
\RULE EVENING       UNFINISHED    SILENT        * LIKELY
\RULE EVENING       UNFINISHED    NOISY         * UNCERTAIN
