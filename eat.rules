\START-GROUP TIME
  MIDNIGHT
  MORNING
  MIDDAY
  AFTERNOON
  EVENING
\END-GROUP

\START-GROUP DURATION
  FLASHING
  FAST
  SHORT
  AVERAGE
  LONG
  EXTENSIVE
  UNFINISHED
\END-GROUP

\START-GROUP VARIETY
  NONE
  POOR
  AVERAGE
  RICH
\END-GROUP

\START-GROUP NOISE
  SILENT
  NOISY
  TURBULENT
\END-GROUP

\START-GROUP CONFIDENCE
  ILLOGIC
  UNLIKELY
  UNCERTAIN
  LIKELY
  CERTAIN
\END-GROUP

\START-MAPPING
  TIME
  DURATION
  NOISE
  VARIETY
  \INTO CONFIDENCE
\END-MAPPING

\RULE * FLASHING   *         *       ILLOGIC
\RULE * EXTENSIVE  *         *       ILLOGIC
\RULE * FAST       *         NONE    LIKELY
\RULE * FAST       SILENT    POOR    LIKELY
\RULE * FAST       NOISY     POOR    UNCERTAIN
\RULE * FAST       TURBULENT POOR    UNCERTAIN
\RULE * FAST       SILENT    AVERAGE LIKELY
\RULE * FAST       NOISY     AVERAGE LIKELY
\RULE * FAST       TURBULENT AVERAGE UNLIKELY
\RULE * FAST       SILENT    RICH    UNCERTAIN
\RULE * FAST       NOISY     RICH    UNLIKELY
\RULE * FAST       TURBULENT RICH    UNLIKELY
\RULE * SHORT      *         NONE    LIKELY
\RULE * SHORT      SILENT    POOR    LIKELY
\RULE * SHORT      NOISY     POOR    LIKELY
\RULE * SHORT      TURBULENT POOR    UNCERTAIN
\RULE * SHORT      SILENT    AVERAGE LIKELY
\RULE * SHORT      NOISY     AVERAGE UNCERTAIN
\RULE * SHORT      TURBULENT AVERAGE UNLIKELY
\RULE * SHORT      SILENT    RICH    UNCERTAIN
\RULE * SHORT      NOISY     RICH    UNLIKELY
\RULE * SHORT      TURBULENT RICH    UNLIKELY
\RULE * AVERAGE    *         NONE    CERTAIN
\RULE * AVERAGE    SILENT    POOR    LIKELY
\RULE * AVERAGE    NOISY     POOR    LIKELY
\RULE * AVERAGE    TURBULENT POOR    UNCERTAIN
\RULE * AVERAGE    SILENT    AVERAGE LIKELY
\RULE * AVERAGE    NOISY     AVERAGE LIKELY
\RULE * AVERAGE    TURBULENT AVERAGE UNCERTAIN
\RULE * AVERAGE    SILENT    RICH    UNCERTAIN
\RULE * AVERAGE    NOISY     RICH    UNLIKELY
\RULE * AVERAGE    TURBULENT RICH    ILLOGIC
\RULE * LONG       *         NONE    CERTAIN
\RULE * LONG       *         POOR    CERTAIN
\RULE * LONG       SILENT    AVERAGE CERTAIN
\RULE * LONG       NOISY     AVERAGE LIKELY
\RULE * LONG       TURBULENT AVERAGE UNCERTAIN
\RULE * LONG       SILENT    RICH    LIKELY
\RULE * LONG       NOISY     RICH    UNCERTAIN
\RULE * LONG       TURBULENT RICH    UNLIKELY
\RULE * UNFINISHED SILENT    NONE    UNLIKELY
\RULE * UNFINISHED NOISY     NONE    UNCERTAIN
\RULE * UNFINISHED TURBULENT NONE    UNLIKELY
\RULE * UNFINISHED SILENT    POOR    UNLIKELY
\RULE * UNFINISHED NOISY     POOR    UNCERTAIN
\RULE * UNFINISHED TURBULENT POOR    UNLIKELY
\RULE * UNFINISHED SILENT    AVERAGE UNLIKELY
\RULE * UNFINISHED NOISY     AVERAGE UNLIKELY
\RULE * UNFINISHED TURBULENT AVERAGE UNLIKELY
\RULE * UNFINISHED SILENT    RICH    UNLIKELY
\RULE * UNFINISHED NOISY     RICH    UNLIKELY
\RULE * UNFINISHED TURBULENT RICH    UNLIKELY
